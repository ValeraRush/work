//В функцию передаем массив. Нужно скопировать значения с одного массива в другой. В скопированном массиве изменить любой элемент и вывести 2 массива.
let arr=[1,2,3,4,5,6]
let arr1=[7,8,9,5,4,3]
let testFunction=(arr,arr1)=>{
	arr1.forEach(item=>arr.push(item))
	return arr
}
console.log(testFunction(arr,arr1))
arr1[2]=4
console.log(arr1)
